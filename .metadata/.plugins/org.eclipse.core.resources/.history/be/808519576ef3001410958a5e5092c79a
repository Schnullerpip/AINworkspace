package testPackage;

public class TestClass {
	
	public TestClass(int val){
		this.a = val;
		System.out.println("Hi im a newTestClass-Object, my value is: " + this.a);
	}
	
	
	public static void main(final String[] args){
		int b = 3, c = 4;
		double d = 1.1;
		TestClass a = new TestClass(0);
		TestClass b = new TestClass(1);
		TestClass c = new TestClass(2);
		TestClass d = new TestClass(3);
		
	}
}
