package testPackage;

import javax.swing.JComponent;

public class TileComponent extends JComponent{
	
	private static int sizeX = 108, sizeY = sizeX;
	
	private Game game;
	private int x, y;
	double b = 1.5;
	
	public TileComponent(Game g, int x, int y){
		game = g;
		this.x = x;
		this.y = y;
		
		super.setSize(int b, sizeY);
	}
	
	
	
	

}
